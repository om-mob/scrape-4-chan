doctype html
html(lang="en")
  head
    title= pageTitle
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width,initial-scale=1")

    style !{styleString}

  body.is_thread.board_gif.hasDropDownNav.yotsuba_new.nws.compact
    .boardBanner
      #bannerCnt.title.desktop(data-src="")
        img(src="data:image/jpg;base64,", alt="4chan")
      
      .boardTitle= board
    hr.abovePostForm

    .nav-links.desktop
      span [
      a(href="#bottom") Bottom 
      span ] [
      a(href= thread_url) Thread URL
      span ]
      .thread-stats 
        span.ts-replies(data-tip="Replies") #{thread_stats.replies} / 
        span.ts-images(data-tip="Images") #{thread_stats.images} / 
        span #{thread_stats.total_size}
    hr


    form#delform
      .board 
        .thread 
          //- Op
          - var postId = posts[0].post_id.slice(1)
          div(id= `pc${postId}`).postContainer.opContainer
            div(id= `p${postId}`).post.op
              div(id= `f${postId}`).file
                div(id= `fT${postId}`).fileText
                  span File:
                  a(href= `${posts[0].image.videoUrl}`, target="blank") #{posts[0].image.title} 
                  span (#{posts[0].image.size}, #{posts[0].image.resolution})
                a.fileThumb(href= ``, target="_blank")
                  img(src= `data:image/jpg;base64,${posts[0].image.thumbnailBase64}`, alt= `${posts[0].image.size}`, loading="lazy", data-md5)
              
              div(id= `pi${postId}`).postInfo.desktop !{posts[0].full_body.post_info}
                a.postMenuBtn(href="#", title="Post menu", data-cmd="post-menu") ▶
                div(id= `bl_${postId}`).backlink
                  each reply in posts[0].replies
                    span 
                      a.quotelink(href= `#p${reply}`) &gt;&gt;#{reply} 
                      //- &nbsp;

              blockquote(id= `m${postId}`).postMessage #{posts[0].body}

          //- Posts
          mixin postMixin(postObj)
            - var postId = postObj.post_id.slice(1)
            div(id= `pc${postId}`).postContainer.replyContainer
              div(id= `p${postId}`).post.reply
                div(id= `pi${postId}`).postInfo.desktop !{postObj.full_body.post_info}
                  a.postMenuBtn(href="#", title="Post menu", data-cmd="post-menu") ▶
                  div(id= `bl_${postId}`).backlink
                    each reply in postObj.replies
                      span
                        a.quotelink(href= `#p${reply}`) &gt;&gt;#{reply} 

                if (postObj.image.title)
                  div(id= `f${postId}`).file
                    div(id= `fT${postId}`).fileText
                      span File:
                      a(href= `${postObj.image.videoUrl}`, target="blank") #{postObj.image.title} 
                      span (#{postObj.image.size}, #{postObj.image.resolution})
                    a.fileThumb(href= ``, target="_blank")
                      img(src= `data:image/jpg;base64,${postObj.image.thumbnailBase64}`, alt= `${postObj.image.size}`, loading="lazy", data-md5)

                blockquote(id= `m${postId}`).postMessage !{postObj.full_body.blockquote}

          each post in posts.slice(1)
            +postMixin(post)
          
        
        //- NAV
        hr
        .navLinks.navLinksBot.desktop
          span [
          a(href="#top") Top 
          span ] [
          a(href= thread_url) Thread URL
          span ]
          .thread-stats 
            span.ts-replies(data-tip="Replies") #{thread_stats.replies} / 
            span.ts-images(data-tip="Images") #{thread_stats.images} / 
            span #{thread_stats.total_size}
        hr.desktop

  

    script.
      const quoteLinks = document.querySelectorAll(".quotelink")
      const mouseoverHandler = (e) => {
        if (e.target.classList.contains("linkfade")) return
        const quoteId = e.target.getAttribute("href").slice(2)
        const rect = e.target.getBoundingClientRect()

        const replyElement = document.querySelector(`#p${quoteId}`)
        const bounding = replyElement.getBoundingClientRect()
        const replyElementWithinViewport = bounding.top >= 0 && bounding.left >= 0 && bounding.right <= window.innerWidth && bounding.bottom <= window.innerHeight

        if (replyElementWithinViewport) return replyElement.classList.add("highlight")

        const quotePreviewElement = document.createElement("div")
        quotePreviewElement.setAttribute("id", "quote-preview")
        quotePreviewElement.classList.add("post", "reply", "preview")
        quotePreviewElement.style.left = `${rect.left + window.scrollX + rect.width}px`
        quotePreviewElement.style.top = `${rect.top + window.scrollY - rect.height}px`
        quotePreviewElement.innerHTML = replyElement.innerHTML

        document.body.append(quotePreviewElement)


        //- console.log(replyElement)
      }

      const mouseleaveHandler = (e) => {
        const quoteId = e.target.getAttribute("href").slice(2)
        const replyElement = document.querySelector(`#p${quoteId}`)
        const quotePreview = document.querySelector("#quote-preview")
        if (replyElement.classList.contains("highlight")) return replyElement.classList.remove("highlight")
        document.body.removeChild(quotePreview)
      }

      quoteLinks.forEach(quoteLink => {
        quoteLink.addEventListener("mouseover", mouseoverHandler)
        quoteLink.addEventListener("mouseleave", mouseleaveHandler)
      })

    script.
      const threadId = document.querySelector(".post.op").getAttribute("id").slice(1)
      const clickHandler = (e) => {
        e.preventDefault()
        const quoteId = e.target.getAttribute("href").slice(2)
        if (quoteId === threadId) return

        const replyElement = document.querySelector(`#p${quoteId}`)
        const replyClone = replyElement.cloneNode(true)
        replyClone.id = "clone-" + replyClone.id
        replyClone.classList.remove("highlight")
        const currentBlockquote = e.target.parentNode.parentNode.parentNode.parentNode.querySelector("blockquote")

        if (e.shiftKey) {
          e.preventDefault()
          replyElement.scrollIntoView({behavior: 'smooth'});
          return
        }
        e.preventDefault()

        if (e.target.classList.contains("linkfade")) { // Restore to Original
          if(e.target.matches("blockquote .quotelink")) { 
            replyElement.style.display = ""
            e.target.parentNode.removeChild(e.target.nextSibling)
          }

          e.target.classList.remove("linkfade")
          currentBlockquote.removeChild(currentBlockquote.querySelector(`#clone-p${quoteId}`))
          replyElement.style.display = ""
          return
        }

        e.target.classList.add("linkfade")
        if(e.target.matches("blockquote .quotelink")) {
          //- replyElement.style.display = "none"
          e.target.parentNode.insertBefore(replyClone, e.target.nextSibling);
          return
        }

        
        currentBlockquote.prepend(replyClone)
        replyElement.style.display = "none"
      }
      quoteLinks.forEach(quoteLink => {
        quoteLink.addEventListener("click", clickHandler)
      })

      document.querySelectorAll("blockquote a.quotelink").forEach(el => {
        if (el.textContent.slice(2) === threadId) el.textContent += " (OP)" 
      })